{
  "id": "com.yordex.stripe.app",
  "version": "0.0.2",
  "name": "Yordex",
  "icon": "",
  "permissions": [
    {
      "permission": "secret_write",
      "purpose": "Allows storing secrets between page reloads"
    },
    {
      "permission": "secret_read",
      "purpose": "Allows get secrets between page reloads"
    }
  ],
  "app_backend": {
    "webhooks": null
  },
  "ui_extension": {
    "views": [
      {
        "viewport": "stripe.dashboard.drawer.default",
        "component": "App"
      }
    ],
    "actions": [],
    "content_security_policy": {
      "connect-src": [
        "https://new-app.dev.yordex.com/api/sessions"
      ],
      "image-src": [
        "https://blocventures.com/wp-content/uploads/2020/03/yordex_1.png"
      ],
      "purpose": ""
    }
  },
  "post_install_action": null
}